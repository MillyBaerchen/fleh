{
	"name": "FL Enhanced",
	"description": "Fliplife Enhanced for Google Chrome",
	"version": "0.10.0",
	"icons": {
		"16": "icon/16.png",
		"48": "icon/48.png",
		"128": "icon/128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"http://fliplife.com/*"
			],
			"css": [
				"css/fleh.css"
			],
			"js": [
				"js/mootools/core.js",
				"js/mootools/more.js",
				"js/fleh.js",
				"js/fleh/log.js",
				"js/fleh/autopilot.js",
				"js/fleh/values.js",
				"js/fleh/tools.js",
				"js/fleh/worker.js",
				"js/fleh/worker/career.js",
				"js/fleh/worker/home.js",
				"js/fleh/worker/project.js",
				"js/fleh/worker/shopping.js",
				"js/fleh/worker/sparetime.js",
				"js/fleh/worker/sparetime/party.js",
				"js/init.js"
			],
			"run_at": "document_idle"
		}
	],
	"permissions": [
		"http://fliplife.com/"
	]
}
